#include <stdint.h>
#include <cmath>
#include <time.h>
#include <iostream>
using namespace std;

int main(){
    clock_t start, end;
    int64_t cpu_time_used;
    start = clock();
    int a = pow(10,11);
    float n;
    for (int32_t i = 0; i < a; i++){
      n = 2.0 + 2.0;
    }
   int b = 5 * pow(10, 10);
    float m;
    for (int32_t i = 0; i < b; i++){
      m = 2 * 2;
    }
    end = clock();
    cpu_time_used = ((double)(end - start))/ CLOCKS_PER_SEC;
		cout << "Time Used\n" << cpu_time_used;
    return 0;
}
